    "circular-dependency-plugin": "^5.2.0",
    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "4.0.0",
    "cross-env": "7.0.2",
    "css-loader": "3.5.3",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "7.2.0",
